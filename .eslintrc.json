{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [],
  "rules": {
    "no-unused-vars": "warn",
    "no-undef": "warn"
  },
  "overrides": [
    {
      "files": ["cypress/**/*.{js,ts,jsx,tsx}"],
      "extends": ["plugin:cypress/recommended"],
      "plugins": ["cypress"]
    }
  ]
}
